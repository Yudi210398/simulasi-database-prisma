generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Produk {
  id          Int    @id @default(autoincrement())
  namaBarang  String @db.VarChar(100)
  hargaBarang Int    @db.Integer
  qty         Int    @db.Integer

  @@map("produk")
}

model Customer {
  id   Int    @id @default(autoincrement())
  nama String @db.VarChar(100)
  umur Int    @db.Integer

  @@map("customer")
}
